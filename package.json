{
    "name": "solfegejs-server",
    "description": "Server bundle of SolfegeJS",
    "version": "2.1.3",
    "author": "neolao <contact@neolao.com>",
    "repository": "https://github.com/neolao/solfege-bundle-server.git",
    "license": "MIT",
    "main": "lib/Bundle.js",
    "engines": {
        "node": ">= 6"
    },
    "files": [
        "lib",
        "src",
        "interface.js"
    ],
    "peerDependencies": {
        "solfegejs": "2.x"
    },
    "dependencies": {
        "accepts": "1.3.*",
        "bind-generator": "^1.0.0",
        "co": "4.5.*",
        "formidable": "1.0.*",
        "is-generator": "^1.0.3",
        "raw-body": "^2.1.*"
    },
    "devDependencies": {
        "babel-cli": "^6.7",
        "babel-preset-solfege": "^1.0.1",
        "flow-bin": "^0.47",
        "gnomon": "^1.5.0",
        "watch": "^1.0.2"
    },
    "scripts": {
        "flow-stop": "flow stop",
        "flow-start": "flow start",
        "flow-restart": "flow stop; flow start",
        "prepare": "npm run build",
        "prebuild": "flow",
        "build": "babel --out-dir lib src --copy-files",
        "watch": "watch 'echo \"----------\"; npm run build --silent | gnomon' ./src",
        "example-build": "flow; babel --out-dir example/lib example/src --copy-files",
        "example-console": "node example/lib/console.js"
    },
    "babel": {
        "presets": [
            "solfege"
        ],
        "comments": false
    }
}
