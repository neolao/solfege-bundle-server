{
    "name"          : "solfegejs-server",
    "description"   : "Server bundle of SolfegeJS",
    "version"       : "1.6.2",
    "author"        : "neolao <contact@neolao.com>",
    "repository"    : "https://github.com/neolao/solfege-bundle-server.git",
    "license"       : "MIT",
    "main"          : "bundle/index.js",
    "engines"       : 
    {
        "node": ">= 0.12"
    },
    "dependencies"  :
    {
        "solfegejs"     : "1.4.*",
        "co"            : "4.5.*",
        "raw-body"      : "2.1.2",
        "formidable"    : "1.0.*"
    },
    "devDependencies":
    {
        "babel"         : "5.8.*",
        "babel-plugin-object-assign" : "1.2.*",
        "chai"          : "2.3.*",
        "mocha"         : "2.2.*"
    },
    "scripts"       :
    {
        "test"             : "mocha --harmony --harmony-proxies --recursive --reporter spec --bail",
        "es6-watch"        : "babel --blacklist=regenerator --plugins=object-assign --watch --out-dir bundle bundle-es6",
        "es6-compile"      : "babel --blacklist=regenerator --plugins=object-assign --out-dir bundle bundle-es6",
        "example-compile"  : "babel --blacklist=regenerator --out-dir example/bundle example/bundle-es6",
        "example-start"    : "node --harmony --harmony-proxies example/bundle/start.js"
    }

}
